<Window x:Class="WpfTechPharma.Janelas.JanListarFornecedor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfTechPharma.Janelas"
        xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        WindowStartupLocation="CenterScreen"
        Height="470" Width="840"
        MinHeight="470" MinWidth="840"
        Title="Listar Fornecedores" 
        ResizeMode="NoResize"
        TextElement.FontWeight="Medium"
        FontFamily="{materialDesign:MaterialDesignFont}"
        FontSize="16">
    <Grid>
        
        <DataGrid d:ItemsSource="{d:SampleData ItemCount=5}" BorderThickness="3" BorderBrush="#FF320B86"
                  HorizontalGridLinesBrush="#FFDA1010" GridLinesVisibility="All" Foreground="#00000000" FontSize="16"/>
        
    </Grid>
</Window>
